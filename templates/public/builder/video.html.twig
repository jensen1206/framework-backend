<div id="{% if f.config.container_id %}{{ f.config.container_id }}{% else %}{{ f.id }}{% endif %}"
     class="{{ f.config.css_class }}">
    <div data-container="{{ f.id }}" data-video="{{ f.config.videos|base64Array }}" data-options="{{ f.config.carouselOptions|base64Array }}" class="app-video video-{{ f.id }} {% if f.config.carousel %}app-video-carousel{% endif %}">
        <div id="app-video-{{ f.id }}"
             class="blueimp-gallery blueimp-gallery-carousel blueimp-gallery-svgasimg blueimp-gallery-smil blueimp-gallery-display blueimp-gallery-controls"
             aria-label="video gallery"
             aria-modal="false"
             role="dialog">
            <div class="slides" aria-live="polite"></div>
            <h3 class="title"></h3>
            <p class="description"></p>
            <a
                    class="prev"
                    aria-controls="blueimp-gallery"
                    aria-label="previous slide"
                    aria-keyshortcuts="ArrowLeft"
            ></a>
            <a
                    class="next"
                    aria-controls="blueimp-gallery"
                    aria-label="next slide"
                    aria-keyshortcuts="ArrowRight"
            ></a>
            <a
                    class="play-pause"
                    aria-controls="blueimp-gallery"
                    aria-label="play slideshow"
                    aria-keyshortcuts="Space"
                    aria-pressed="false"
                    role="button"
            ></a>
            <ol class="indicator"></ol>
        </div>
    </div>
</div>